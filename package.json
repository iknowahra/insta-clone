    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "prettier-eslint": "11.0.0",
    "prisma": "2.17.0"
  }
}
